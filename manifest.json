{
	"name": "PrimeMate",
	"version": "1.0",
	"description": "Primer Generator for Ensembl",
	"permissions": ["tabs"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js", "libraries/jquery-3.5.1.min.js"]
		}
	],
	"icons": {
		"128": "assets/icon128.png",
		"48": "assets/icon48.png"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"manifest_version": 2
}
